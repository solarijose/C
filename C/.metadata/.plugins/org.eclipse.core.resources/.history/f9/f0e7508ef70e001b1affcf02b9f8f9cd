#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int numero1, numero2, numero3, numero4, numero5, suma;
	printf("Ingresar 5 numeros\n");
	printf("Ingresar numero\n");
	scanf("%d\n",&numero1);
	printf("Ingresar numero\n");
	scanf("%d\n",&numero2);
	printf("Ingresar numero\n");
	scanf("%d\n",&numero3);
	printf("Ingresar numero\n");
	scanf("%d\n",&numero4);
	printf("Ingresar numero\n");
	scanf("%d\n",&numero5);

	suma=numero1+numero2+numero3+numero4+numero5;
	printf("la suma es %d\n", &suma);

	return 0; //indica el fin del progama
}
